# Discord Bot Token
TOKEN=your_discord_bot_token

# Discord Bot Client/Application ID
CLIENT_ID=your_discord_bot_client_id

# VietQR API credentials  
VIETQR_CLIENT_ID=your_vietqr_client_id
VIETQR_API_KEY=your_vietqr_api_key

# Casso API credentials
# Lấy từ: https://app.casso.vn/setting/api
CASSO_API_KEY=AK_CS.xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Bank ID - ID của tài khoản ngân hàng trong Casso
# Lấy từ: https://app.casso.vn hoặc chạy get-bank-id.js
CASSO_BANK_ID=12981

# Server Port
PORT=3000
